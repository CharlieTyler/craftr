<div class = "container">
  <div class = "row">
    <div class = "col-12">
      <h4>Select a user</h4>
      <p class = "text-muted">Sorted by recency of creation please use <strong>ctrl+f</strong> for now - search function on list for development</p>
      <ul>
        <% @users.each do |user| %>
          <li><%= link_to user.email, edit_admin_user_path(user) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
